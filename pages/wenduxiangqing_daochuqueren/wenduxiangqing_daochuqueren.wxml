        />
      </view>
      <view class="flex-col mt-17">
        <view class="flex-col section_5">
          <text class="self-start font_2 text_2">2024-05-16</text>
          <view class="flex-col self-stretch mt-25">
            <view class="flex-row items-center">
              <text class="font_3">平均温度：</text>
              <text class="ml-4 font_4 text_3">22℃</text>
            </view>
            <view class="flex-row mt-13">
              <view class="flex-row justify-evenly items-center section_6 equal-division-item">
                <text class="font_3">最高温度：</text>
                <text class="font_5 text_4">32℃</text>
              </view>
              <view class="flex-row justify-evenly items-center section_6 equal-division-item_2 ml-15">
                <text class="font_3">最低温度：</text>
                <text class="font_5 text_5">12℃</text>
